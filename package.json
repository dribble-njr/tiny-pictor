{
  "name": "tiny-pictor",
  "version": "0.0.2",
  "description": "A tiny image compress cross-platform cli based on sharp",
  "bin": {
    "tp": "./bin/cli.js"
  },
  "scripts": {
    "test": "test",
    "build": "tsup",
    "dev": "tsup --watch",
